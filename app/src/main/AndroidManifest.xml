<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="revanth.language">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".NumberLayout" android:label="Numbers"></activity>
        <activity android:name=".FamilyLayout" android:label="Family"></activity>
        <activity android:name=".ColorsLayout" android:label="Colors"></activity>
        <activity android:name=".PhrasesLayout" android:label="Phrases"></activity>
    </application>

</manifest>